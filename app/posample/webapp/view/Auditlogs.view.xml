<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="ust.po.sample.posample.controller.Auditlogs">

    <Page id="AuditlogsDataPage" title="Audit Logs Data" >
        <content>

            <Table id="auditlogsTable" inset="false" items="{audModel>/POAudits}">
                <columns>
                    <Column id="_IDGenColumn22"><Text id="_IDGenText13" text="PO ID" /></Column>
                    <Column id="_IDGenColumn16"><Text id="_IDGenText23" text="Error Status" /></Column>
                    <Column id="_IDGenColumn25"><Text id="_IDGenText34" text="Audit Status" /></Column>
                    <Column id="_IDGenColumn31"><Text id="_IDGenText45" text="Audit Log AuditedBy" /></Column>
                </columns>

                <items>
                    <ColumnListItem id="_IDGenColumnListItem3">
                        <cells>
                            <Text id="_IDGenText53" text="{audModel>error_po}" />
                            <Text id="_IDGenText63" text="{audModel>error_status}" />
                            <Text id="_IDGenText73" text="{audModel>audit_status}" />
                            <Text id="_IDGenText83" text="{audModel>audit_log_auditby}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>

        </content>
    </Page>

</mvc:View>
