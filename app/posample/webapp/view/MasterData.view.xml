<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="ust.po.sample.posample.controller.MasterData"
>
    <Page
        id="MasterDataPage"
        title="PO Data"
    >
        <content>
            <Table
                id="materialTable"
                inset="false"
                items="{/Materials}"
            >
                <columns>
                    <Column id="_IDGenColumn33">
                        <Text
                            id="_IDGenText17"
                            text="Material Code"
                        />
                    </Column>
                    <Column id="_IDGenColumn17">
                        <Text
                            id="_IDGenText27"
                            text="Material Description"
                        />
                    </Column>
                    <Column id="_IDGenColumn27">
                        <Text
                            id="_IDGenText37"
                            text="GST Number"
                        />
                    </Column>
                    <Column id="_IDGenColumn37">
                        <Text
                            id="_IDGenText47"
                            text="Active Status"
                        />
                    </Column>
                </columns>

                <items>
                    <ColumnListItem
                        id="_IDGenColumnListItem6"
                        type="Navigation"
                        press="materialToPoItem"
                    >
                        <cells>
                            <Text
                                id="_IDGenText55"
                                text="{mm_code}"
                            />
                            <Text
                                id="_IDGenText65"
                                text="{mm_desc}"
                            />
                            <Text
                                id="_IDGenText75"
                                text="{mm_gstno}"
                            />
                            <Text
                                id="_IDGenText85"
                                text="{is_Active}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
