{
  "_version": "1.76.0",
  "sap.app": {
    "id": "ust.po.sample.posample",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "bca2bc96-b501-41f3-9398-d0e01551729f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/md/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "poService": {
        "uri": "/odata/v2/po/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "grService": {
        "uri": "/odata/v2/gr/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "invService": {
        "uri": "/odata/v2/inv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "audService": {
        "uri": "/odata/v2/aud/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ust.po.sample.posample.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "poModel": {
        "dataSource": "poService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "grModel": {
        "dataSource": "grService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "invModel": {
        "dataSource": "invService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "audModel": {
        "dataSource": "audService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ust.po.sample.posample.view",
        "async": true,
        "viewPath": "ust.po.sample.posample.view"
      },
      "routes": [
        {
          "name": "RouteDefault",
          "pattern": ":?query:",
          "target": [
            "TargetDefault"
          ]
        },
        {
          "name": "MasterData",
          "pattern": "tomasterdata",
          "target": "MasterData"
        },
        {
          "name": "MaterialDetail",
          "pattern": "materials/{materialId}",
          "target": "MaterialDetail"
        },
        {
          "name": "POData",
          "pattern": "topodata",
          "target": "POData"
        },
        {
          "name" : "GRData",
          "pattern": "togrdata",
          "target": "GRData"
        },
        {
          "name": "INVData",
          "pattern": "toinvdata",
          "target": "INVData"
        },
        {
          "name": "AuditLogs",
          "pattern": "toauditlogs",
          "target": "AuditLogs"
        }
      ],
      "targets": {
        "TargetDefault": {
          "id": "Default",
          "name": "Default"
        },
        "MasterData": {
          "id": "Master",
          "name": "MasterData"
        },
        "MaterialDetail" :{
          "id": "MaterialDetail",
          "name": "MaterialDetail"
        },
        "POData": {
          "id": "podata",
          "name": "POData"
        },
        "GRData" :{
          "id": "grdata",
          "name": "GRData"
        },
        "INVData" : {
          "id": "invdata",
          "name": "INVData"
        },
        "AuditLogs":{
          "id": "auditlogs",
          "name": "Auditlogs"
        }
      }
    },
    "rootView": {
      "viewName": "ust.po.sample.posample.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}